{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "1.1.0",
  "author": "relexx",
  "default_locale": "en",
  "browser_specific_settings": {
    "gecko": {
      "id": "tb-seafile-filelink@relexx.de",
      "strict_min_version": "128.0"
    }
  },
  "icons": {
    "16": "icons/seafile-16.png",
    "32": "icons/seafile-32.png",
    "64": "icons/seafile-64.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "cloud_file": {
    "name": "Seafile",
    "management_url": "management.html",
    "reuse_uploads": false
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "experiment_apis": {
    "loginManager": {
      "schema": "experiment_apis/loginManager/schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "paths": [
          [
            "loginManager"
          ]
        ],
        "script": "experiment_apis/loginManager/implementation.js"
      }
    }
  }
}